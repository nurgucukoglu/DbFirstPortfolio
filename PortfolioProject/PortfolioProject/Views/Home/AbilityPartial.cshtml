@model List<PortfolioProject.Models.AbilityTbl>
@{
    ViewBag.Title = "AbilityPArtial";
    Layout = "~/Views/Shared/_PortfolioLayout.cshtml";
}

<!-- home page about section -->

    <section class="w3l-homeblock1" id="about">
        <div class="midd-w3 py-5">
            <div class="container py-lg-5 py-md-3">
                <div class="row">
                    <div class="col-lg-6">
                        <span class="text">My Services</span>
                        <h3 class="title-big">Bunlar benim yeteneklerim: </h3>
                       
                        @foreach (var item in Model)
                        {
                            <ul class="service-list mt-md-5 mt-4">
                                <li class="service-link"><a href="#">@item.AbilityName</a></li>
                            </ul>
                        }
                        </div>
                    <div class="col-lg-6 mt-lg-0 mt-5">
                        <div class="position-relative">
                            <img src="~/Content/iportfolio-starter/assets/images/kod.png" alt="" class="radius-image img-fluid">
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>


<!-- //home page about section -->
